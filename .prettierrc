{
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none",
    "printWidth": 80,
    "tabWidth": 4,
    "endOfLine": "lf",
    "arrowParens": "avoid",
    "plugins": ["prettier-plugin-sort-imports"],
    "prettier-plugin-sort-imports": {
        "sortImports": {
            "order": "asc",
            "separateGroups": false
        },
        "spaceBetweenBraces": false
    }
}
